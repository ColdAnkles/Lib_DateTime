[h: calendar = getLibProperty("calendarData","Lib:DateTime")]
[h: currentYear = json.get(calendar,"calendarYear") - 1]
[h: calendar=json.set(calendar,"calendarYear",currentYear)]
[h: calendar=json.set(calendar,"yearStartDay",json.get(calendar,"lastYearStartDay"))]
[h: calendar=json.set(calendar,"nextYearStartDay",json.get(calendar,"yearStartDay"))]
[h: dayInYear = 0 ]
[h: daysPerMonth = json.get(calendar,"dayInMonth")]
[h: monthCount = json.length(json.get(calendar,"months"))]
[h: dayInYear = math.arraySum(daysPerMonth)]
[h: lastStartDay = math.mod(json.get(calendar,"dayInWeek")+json.get(calendar,"yearStartDay")-math.mod(dayInYear, json.get(calendar,"dayInWeek")),json.get(calendar,"dayInWeek"))]
[h: calendar = json.set(calendar,"lastYearStartDay",lastStartDay)]
[h: setLibProperty("calendarData", calendar,"Lib:DateTime")]